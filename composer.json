{
  "name": "siaoynli/phone-auth-plugin",
  "description": "基于 Laravel 11 的手机验证码登录插件包",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Siaoynli",
      "email": "siaoynli@126.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "laravel/framework": "^11.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "laravel/pint": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Siaoynli\\PhoneAuth\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Siaoynli\\PhoneAuth\\Tests\\": "tests/"
    }
  },
  "extra": {
    "plugin": {
      "class": "Siaoynli\\PhoneAuth\\PhoneAuthPlugin"
    },
    "laravel": {
      "providers": [
        "Siaoynli\\PhoneAuth\\Providers\\PhoneAuthServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}